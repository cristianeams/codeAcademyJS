let randomNumbers = [375, 200, 3.14, 7, 13, 852];

// Call .filter() on randomNumbers below
//return values < 250
let smallNumbers = randomNumbers.filter(function(number) {
  return number < 250;
  
});
console.log(smallNumbers);

//arow syntax
let smallNumbersArrow = randomNumbers.filter(number => number < 250);

console.log(smallNumbersArrow);

let favoriteWords = ['nostalgia', 'hyperbole', 'fervent', 'esoteric', 'serene'];

let longFavoriteWords = favoriteWords.filter (word) {
  return word.length > 7;
});

// Refactor the code above using arrow function syntax

let longFavoriteWordsArrow = favoriteWords.filter(word => word.length > 7);
console.log(longFavoriteWordsArrow);
