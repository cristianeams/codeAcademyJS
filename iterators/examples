let cities = ['Nashville', 'Charlotte', 'Asheville', 'Austin', 'Boulder'];

let nums = [1, 50, 75, 200, 350, 525, 1000];

// a method that returns undefined and does not change the array = forEach 
cities.forEach(city => console.log('Have you visited ' + city + '?'));

//a method that returns a new array with elements that meet this condition = filter
let longCities = cities.filter(city => city.length > 7);

//a method that returns a new array with updated elements = map
let smallerNums = nums.map(num => num - 5);

//a method that returns a boolean value = every or some
nums.some(num => num < 0);
